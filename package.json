        "ssg",
        "website",
        "static-site-generator",
        "jamstack",
        "frontmatter",
        "static-site"
    ],
    "author": {
        "name": "Joshua Sosso",
        "email": "josh@modiimedia.com",
        "url": "https://www.modiimedia.com"
    },
    "scripts": {
